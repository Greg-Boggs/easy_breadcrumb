<?php

/**
 * @file
 * Install hooks
 */

require_once 'includes/EasyBreadcrumbConstants.inc';

/**
 * Implements hook uninstall
 */
function easy_breadcrumb_uninstall() {

  db_query("DELETE FROM {variable} WHERE name LIKE '" .
    EasyBreadcrumbConstants::DB_VARS_PREFIX . "%'");

  cache_clear_all('variables', 'cache');
}
